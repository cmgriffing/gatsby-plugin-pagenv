{
  "name": "gatsby-plugin-pagenv",
  "description": "Inject ENV Vars into Gatsby pageQueries at build time.",
  "author": "Chris Griffing <cmgriffing@gmail.com> (http://chrisgriffing.com)",
  "version": "0.1.0",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "plugin",
    "pagequery",
    "env",
    "vars"
  ],
  "repository": "github:cmgriffing/gatsby-plugin-pagenv",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "bluebird": "^3.7.1",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "semver": "^5.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "babel-preset-gatsby-package": "^0.2.9",
    "cross-env": "^5.2.1",
    "dotenv": "^8.2.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "peerDependencies": {
    "gatsby": "^2.12.0"
  }
}
